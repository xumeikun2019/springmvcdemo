		SELECT c.open_id from c_meeting_user t,sys_department_user a,sys_user_role b,c_user c 
		where t.user=a.department_id and a.user_id=b.user_id and b.user_id=c.id and t.meeting=#{id} and t.type='1' and b.role_id='6178ed4bee1e11e9833900e081bbbd32'
	</select>
	
	<!-- 查询会议地点 -->
	<select id="findhydd" parameterType="map" resultType="java.util.HashMap">
        select * from c_place where id=#{id}
	</select>
	
	<!-- 查询可以节目审核的部门 -->
	<select id="findshqx" parameterType="map" resultType="java.util.HashMap">
        select * from c_jhsh
	</select>
	<!-- 主页节目审批通过 -->
	<update id="jmshtg" parameterType="map">
		update c_jihua t
		set t.ispass='1' where t.id=#{id,jdbcType=VARCHAR}
	</update>
	<!-- 主页节目审批不通过 -->
	<update id="jmshbtg" parameterType="map">
		update c_jihua t
		set t.ispass='0' where t.id=#{id,jdbcType=VARCHAR}
	</update>
	
	<!-- 查询需要删除的会议资料、文件 -->
	<select id="findwjsc" parameterType="map" resultType="java.util.HashMap">
       select t.hyzl from c_meeting_order t where t.time_start <![CDATA[   <=  ]]> (select now()- interval t.zlscsj day) and t.hyzl!=''
	</select>
	
	<!-- 查询会议资料删除时间(天) -->
	<select id="findtslist" parameterType="map" resultType="java.util.HashMap">
        select * from c_data_date
	</select>
</mapper>